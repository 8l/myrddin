use std

const main = {
	var a = [
		3, 5, 4, 9, 7, 2, 6, 0, 1, 8,
	]
	var b = [
		3, 5, 4, 9, 7, 2, 6, 0, 1, 8,
	]
	var c = [
		"a", "aa", "b", "C", "Cc", "cC", "d", "f", "fuckit", "go",
	]

	std.sort(a[:], intcmp)
	for v in a
		std.put("%i\n", v)
	;;
	std.sort(b[:], std.numcmp)
	for v in b
		std.put("%i\n", v)
	;;
	std.sort(c[:], std.strcmp)
	for v in c
		std.put("%s\n", v)
	;;
}

const intcmp = {a, b
	if a < b
		-> `std.Before
	elif a == b
		-> `std.Equal
	else
		-> `std.After
	;;
}

	
