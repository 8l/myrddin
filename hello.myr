use "sys.use"
use "alloc.use"
extern const printf : (str:byte*, ap:... -> void)
const main = {
	var x : byte*[1024]
	var sz
	var i

	for sz = 1; sz < 65536; sz *= 2
		for i = 0; i < 1024; i++
			x[i] = std.bytealloc(sz)
		;;
		for i = 0; i < 1024; i++
			std.bytefree(x[i], sz)
		;;
	;;
	std.write(1, "Hello world\n")
	printf("some string %d %d \n" castto(byte*), 123, 456)
}
